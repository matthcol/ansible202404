
PLAY [Play with Variables] *****************************************************

TASK [Debug text variable] *****************************************************
ok: [host3.localdomain] => {
    "city1": "Paris"
}

TASK [Debug text variables] ****************************************************
ok: [host3.localdomain] => (item=Paris) => {
    "msg": "Paris"
}
ok: [host3.localdomain] => (item=Pau) => {
    "msg": "Pau"
}
ok: [host3.localdomain] => (item=Toulouse) => {
    "msg": "Toulouse"
}

TASK [Debug numeric value] *****************************************************
ok: [host3.localdomain] => {
    "days": 1
}

TASK [Debug numeric value as text] *********************************************
ok: [host3.localdomain] => {
    "msg": [
        "AnsibleUnicode",
        "2",
        "int"
    ]
}

TASK [Compute with numeric variable] *******************************************
ok: [host3.localdomain] => {
    "res": "2"
}

TASK [Computations with numeric variable] **************************************
ok: [host3.localdomain] => (item=2) => {
    "ansible_loop_var": "item",
    "item": "2"
}
ok: [host3.localdomain] => (item=6) => {
    "ansible_loop_var": "item",
    "item": "6"
}
ok: [host3.localdomain] => (item=5) => {
    "ansible_loop_var": "item",
    "item": "5"
}

TASK [Debug booleans variables] ************************************************
ok: [host3.localdomain] => (item=False) => {
    "ansible_loop_var": "item",
    "item": false
}
ok: [host3.localdomain] => (item=True) => {
    "ansible_loop_var": "item",
    "item": true
}
ok: [host3.localdomain] => (item=True) => {
    "ansible_loop_var": "item",
    "item": true
}

TASK [Debug list] **************************************************************
ok: [host3.localdomain] => {
    "cities": [
        "Bordeaux",
        "Lyon",
        "Marseille",
        "Toulouse"
    ]
}

TASK [Debug list 2] ************************************************************
