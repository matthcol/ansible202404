    "ansible_loop_var": "item",
    "item": [
        "Bordeaux",
        "Lyon",
        "Marseille",
        "Toulouse"
    ]
}
ok: [host3.localdomain] => (item=['Bordeaux', 'Lyon', 'Marseille']) => {
    "ansible_loop_var": "item",
    "item": [
        "Bordeaux",
        "Lyon",
        "Marseille"
    ]
}
