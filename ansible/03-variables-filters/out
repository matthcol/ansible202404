    "city_dict": {
        "name": "Toulouse",
        "population": 470000
    }
}

TASK [Debug dictionnary 2] *****************************************************
ok: [host3.localdomain] => {
    "city_dict2": {
        "name": "Toulouse",
        "population": 470000
    }
}

TASK [Debug dictionnary element] ***********************************************
ok: [host3.localdomain] => {
    "city_dict2['name']": "Toulouse"
}

TASK [Debug dictionnary element simplified] ************************************
ok: [host3.localdomain] => {
    "city_dict2.name": "Toulouse"
}

